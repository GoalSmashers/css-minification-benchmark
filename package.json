{
  "name": "css-minification-benchmark",
  "version": "1.0.0",
  "description": "Benchmark of popular CSS minification tools.",
  "author": "Jakub Pawlowicz <jakub@goalsmashers.com>",
  "homepage": "https://github.com/GoalSmashers/css-minification-benchmark",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoalSmashers/css-minification-benchmark.git"
  },
  "bugs": {
    "url": "https://github.com/GoalSmashers/css-minification-benchmark/issues"
  },
  "keywords": [
    "css",
    "minification",
    "benchmark"
  ],
  "scripts": {
    "bench": "node ./bin/bench --html>index.html",
    "check": "npm run lint",
    "lint": "xo",
    "test": "npm run check && npm run bench"
  },
  "bin": {
    "css-minification-benchmark": "bin/bench"
  },
  "main": "lib/bench.js",
  "dependencies": {},
  "devDependencies": {
    "clean-css": "^4.2.1",
    "cli-table": "^0.3.1",
    "colors": "^1.4.0",
    "crass": "^0.12.3",
    "cssnano": "^4.1.10",
    "csso": "^4.0.2",
    "csswring": "^7.0.0",
    "gzip-size": "^5.1.1",
    "q": "^1.5.1",
    "xo": "^0.25.3"
  },
  "files": [
    "bin",
    "data",
    "lib",
    "_config.yml",
    "index.html"
  ],
  "engines": {
    "node": ">=8.10.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "capitalized-comments": "off",
      "curly": "off",
      "import/no-unassigned-import": "off",
      "new-cap": "off",
      "no-eq-null": "off",
      "no-mixed-operators": "off",
      "no-negated-condition": "off",
      "no-restricted-modules": "off",
      "no-unused-vars": [
        "error",
        {
          "caughtErrors": "none"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-const": [
        "error",
        {
          "ignoreReadBeforeAssign": true
        }
      ],
      "promise/prefer-await-to-then": "off",
      "quote-props": [
        "error",
        "consistent"
      ],
      "spaced-comment": "off",
      "unicorn/explicit-length-check": "off",
      "unicorn/filename-case": "off"
    }
  }
}
